<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/allcl"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.main.fragments.profile.photocmp.PhotoAnalyzeFragment">


    <com.avalon.calizer.ui.main.fragments.profile.photocmp.CanvasView
        android:id="@+id/cv_canvas"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/randomDraw"/>
    <TextView
        android:id="@+id/randomDraw"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="ANALYZE PAGE"
        android:fontFamily="@font/ibm_plex_sans_bold"
        android:gravity="center"/>

</androidx.constraintlayout.widget.ConstraintLayout>